  <data name="Delete" xml:space="preserve">
    <value>DELETE tblFreeTicketAndCapRevenueDetail
WHERE key_id = @key_id;</value>
  </data>
  <data name="DeleteRevenue" xml:space="preserve">
    <value>DELETE tblFreeTicketAndCapRevenueDetail
WHERE revenue_id = @revenue_id;</value>
  </data>
  <data name="SelectData" xml:space="preserve">
    <value>SELECT
	ftrd.key_id
	, ftrd.revenue_id
	, ftrd.ticket_cap_id
	, ftrd.price
	, ftrd.quantity
	, ftrd.update_by
	, ftrd.update_date

	, ftc.ticket_cap_name
	, ftc.counting

	, ftcm.available_to
	
	, CASE 
		WHEN rv.revenue_date &gt; ftcm.available_to
			THEN 0
		ELSE ftrd.price * ftrd.quantity
		END AS summary
FROM tblFreeTicketAndCapRevenueDetail AS ftrd
INNER JOIN tblFreeTicketAndCap AS ftc
	ON ftc.ticket_cap_id = ftrd.ticket_cap_id
INNER JOIN tblRevenue AS rv
	ON rv.revenueid = ftrd.revenue_id
	AND rv.check_ticket = 1
INNER JOIN tblFreeTicketAndCapByMovie AS ftcm
	ON ftcm.movie_id = rv.movies_id
	AND ftcm.ticket_cap_id = ftrd.ticket_cap_id
	AND ftcm.theater_id = rv.theater_id
WHERE revenue_id = @revenue_id
ORDER BY
	ftc.counting DESC
	, ftc.ticket_cap_name
	, ftrd.update_date
;</value>
  </data>
  <data name="UpdateInsert" xml:space="preserve">
    <value>UPDATE tblFreeTicketAndCapRevenueDetail
SET quantity = @quantity
	, update_date = @cvDate
	, update_by = @update_by
WHERE revenue_id = @revenue_id
	AND ticket_cap_id = @ticket_cap_id
	AND price = @price;

IF @@ROWCOUNT = 0
BEGIN
	INSERT INTO tblFreeTicketAndCapRevenueDetail
	(
		revenue_id
		, ticket_cap_id
		, price
		, quantity
		, update_by
		, update_date
	)
	VALUES
	(
		@revenue_id
		, @ticket_cap_id
		, @price
		, @quantity
		, @update_by
		, @cvDate
	);
END</value>
