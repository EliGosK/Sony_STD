  <data name="Delete" xml:space="preserve">
    <value>DELETE FROM tblProblemRecord
WHERE revenue_date = @revenue_date
	AND theater_id = @theater_id
	AND movie_id = @movie_id;</value>
  </data>
  <data name="SelectData" xml:space="preserve">
    <value>SELECT pr.revenue_date
	, pr.theater_id
	, pr.movie_id
	, pr.problem
	, pr.update_date
	, pr.update_by
	, t.theater_name
	, c.circuit_name
	, m.movie_nameen + '/' + m.movie_nameth as MovieName
	, u.user_name
FROM tblProblemRecord AS pr
INNER JOIN tblTheater AS t
	ON t.theater_id = pr.theater_id
INNER JOIN tblCircuit AS c
	ON c.circuit_id = t.circuit_id
INNER JOIN tblMovie AS m
	ON m.movie_id = pr.movie_id
INNER JOIN tblUser AS u
	ON u.user_id = pr.update_by</value>
  </data>
  <data name="UpdateInsert" xml:space="preserve">
    <value>UPDATE tblProblemRecord
SET problem = @problem
	, update_date = @cvDate
	, update_by = @update_by
WHERE revenue_date = @revenue_date
	AND theater_id = @theater_id
	AND movie_id = @movie_id;

IF @@ROWCOUNT = 0
BEGIN
	INSERT INTO tblProblemRecord (
		revenue_date
		, theater_id
		, movie_id
		, problem
		, update_date
		, update_by
		)
	VALUES (
		@revenue_date
		, @theater_id
		, @movie_id
		, @problem
		, @cvDate
		, @update_by
		);
END</value>
